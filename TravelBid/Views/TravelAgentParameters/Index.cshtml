@model IEnumerable<TravelBid.Models.TravelAgentParameters>

@{
    ViewData["Title"] = "Index";
}
<div style="height: 7vw"></div>

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.yearsexperience)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ASTACertified)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Specialties)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.PlacesVisited)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DateCreated)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DateLastModified)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.yearsexperience)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ASTACertified)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Specialties)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlacesVisited)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateCreated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateLastModified)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
