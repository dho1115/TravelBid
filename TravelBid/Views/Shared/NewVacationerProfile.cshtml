@model IEnumerable<VacationerModel>

@{
    ViewData["Title"] = "VacationerProfile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Please take a few moments to fill out your profile</h3>

<form method="post" action="/TravelResults/NewTravelRequest">
    <label for="fName">First Name</label><input name="customerFirstName" id="fName" />
    <label for="lName">Last Name</label><input name="customerLastName" id="lName" /><br />
    <label for="email">Email</label><input name="Email" id="email" /><br />
    <label for="destination"><input name="destination" id="destination"/></label>
    <label for="maxbudget"><input name="maxbudget" id="maxbudget" /></label><br />
    <textarea name="additionalInfo"></textarea><br />
    <input type="submit" />
</form>



